# Snowflake Connection Configuration
SNOWFLAKE_USER=your_username_here
SNOWFLAKE_PASSWORD=your_password_here
SNOWFLAKE_ACCOUNT=your_account.region.cloud
SNOWFLAKE_WAREHOUSE=your_warehouse_name
SNOWFLAKE_DATABASE=GLOBAL_AI_SYNC
SNOWFLAKE_SCHEMA=HEALTHCARE_DATA

# Optional: Advanced Settings
SNOWFLAKE_ROLE=AI_SYNC_ROLE
SNOWFLAKE_TIMEOUT=30

# Application Settings
BACKEND_PORT=8001
LOG_LEVEL=INFO

# Note: Copy this file to .env and fill in your actual credentials
# Never commit .env to version control!
